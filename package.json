{
  "name": "careconnect-ai-platform",
  "version": "1.0.0",
  "description": "CareConnect AI Platform - A comprehensive, privacy-first AI companion platform",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "test:axe": "turbo run test:axe",
    "type-check": "turbo run type-check",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "prisma:generate": "turbo run prisma:generate",
    "postinstall": "turbo run postinstall",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health-check": "curl http://localhost:3000/api/health",
    "validate-manifest": "node scripts/validate-manifest.js",
    "vercel-build": "turbo run build"
  },
  "dependencies": {
    "@vitality/ai-service": "workspace:*",
    "@vitality/auth": "workspace:*",
    "@vitality/emotion-detection": "workspace:*",
    "@vitality/voice-processing": "workspace:*",
    "@vitality/design-system": "workspace:*",
    "@vitality/utils": "workspace:*",
    "@vitality/analytics": "workspace:*",
    "@vitality/automation": "workspace:*",
    "@vitality/collaboration": "workspace:*",
    "@vitality/database": "workspace:*",
    "@vitality/document-management": "workspace:*",
    "@vitality/file-storage": "workspace:*",
    "@vitality/financial-tracking": "workspace:*",
    "@vitality/goal-tracking": "workspace:*",
    "@vitality/government-resources": "workspace:*",
    "@vitality/health-tracking": "workspace:*",
    "@vitality/learning-tracking": "workspace:*",
    "@vitality/life-hacks": "workspace:*",
    "@vitality/monitoring": "workspace:*",
    "@vitality/notifications": "workspace:*",
    "@vitality/optimization": "workspace:*",
    "@vitality/security": "workspace:*",
    "@vitality/social-network": "workspace:*",
    "@vitality/testing": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "turbo": "^1.12.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
